        </af:gridRow>
        <af:gridRow marginTop="5px" height="auto" id="gr2">
          <af:gridCell marginStart="5px" width="100%" marginEnd="5px" id="gc2">
            <af:panelTabbed position="above" id="pt1" styleClass="tabel_pannel_tab">
              <af:showDetailItem id="tab1" text="VCTAT RO Status">
                <af:table value="#{bindings.Vctat_RO_Status1.collectionModel}" var="row"
                          rows="#{bindings.Vctat_RO_Status1.rangeSize}"
                          emptyText="#{bindings.Vctat_RO_Status1.viewable ? 'No data to display.' : 'Access Denied.'}"
                          rowBandingInterval="0" scrollPolicy="page" autoHeightRows="0"
                          selectedRowKeys="#{bindings.Vctat_RO_Status1.collectionModel.selectedRow}"
                          selectionListener="#{bindings.Vctat_RO_Status1.collectionModel.makeCurrent}"
                          rowSelection="single" fetchSize="#{bindings.Vctat_RO_Status1.rangeSize}"
                          filterModel="#{bindings.Vctat_RO_Status1Query.queryDescriptor}" filterVisible="true"
                          queryListener="#{bindings.Vctat_RO_Status1Query.processQuery}" varStatus="vs" id="t1"
                          partialTriggers="::b2" >
                  <af:column sortProperty="#{bindings.Vctat_RO_Status1.hints.GrantApprovalDetailId.name}"
                             filterable="true" sortable="true"
                             headerText="Grant Approval Detail ID" id="c1" align="center">
                             <f:facet name="filter">
                                          <af:inputText value="#{vs.filterCriteria.GrantApprovalDetailId}"
                                          id="id4" placeholder="Search ID">
                           </af:inputText>
                             </f:facet>

                    <af:inputText value="#{row.bindings.GrantApprovalDetailId.inputValue}" readOnly="true"
                                  label="#{bindings.Vctat_RO_Status1.hints.GrantApprovalDetailId.label}"
                                  required="#{bindings.Vctat_RO_Status1.hints.GrantApprovalDetailId.mandatory}"
                                  columns="#{bindings.Vctat_RO_Status1.hints.GrantApprovalDetailId.displayWidth}"
                                  maximumLength="#{bindings.Vctat_RO_Status1.hints.GrantApprovalDetailId.precision}"
                                  shortDesc="#{bindings.Vctat_RO_Status1.hints.GrantApprovalDetailId.tooltip}" 
                                  id="it1" contentStyle='text-align: center'>
                      <f:validator binding="#{row.bindings.GrantApprovalDetailId.validator}"/>
                      <af:convertNumber groupingUsed="false"
                                        pattern="#{bindings.Vctat_RO_Status1.hints.GrantApprovalDetailId.format}"/>
                    </af:inputText>
                  </af:column>
                   <af:column sortProperty="#{bindings.Vctat_RO_Status1.hints.FarmerNo.name}" filterable="true"
                             sortable="true" headerText="Farmer No." id="c3" align="center">
                             <f:facet name="filter">
                                          <af:inputText value="#{vs.filterCriteria.FarmerNo}"
                                          id="it5" placeholder="Search Farmer No.">
                           </af:inputText>
                             </f:facet>
                    <af:inputText value="#{row.bindings.FarmerNo.inputValue}"
                                  label="#{bindings.Vctat_RO_Status1.hints.FarmerNo.label}"
                                  required="#{bindings.Vctat_RO_Status1.hints.FarmerNo.mandatory}"
                                  columns="#{bindings.Vctat_RO_Status1.hints.FarmerNo.displayWidth}"
                                  maximumLength="#{bindings.Vctat_RO_Status1.hints.FarmerNo.precision}"
                                  shortDesc="#{bindings.Vctat_RO_Status1.hints.FarmerNo.tooltip}" id="it3">
                      <f:validator binding="#{row.bindings.FarmerNo.validator}"/>
                    </af:inputText>
                  </af:column>
                  <af:column sortProperty="#{bindings.Vctat_RO_Status1.hints.Name.name}" filterable="true"
                             sortable="true" headerText="Farmer" id="c2">
                              <f:facet name="filter">
                                          <af:inputText value="#{vs.filterCriteria.Name}"
                                          id="it4" placeholder="Search Farmer">
                           </af:inputText>
                             </f:facet>
                    <af:inputText value="#{row.bindings.Name.inputValue}"
                                  label="#{bindings.Vctat_RO_Status1.hints.Name.label}"
                                  required="#{bindings.Vctat_RO_Status1.hints.Name.mandatory}"
                                  columns="#{bindings.Vctat_RO_Status1.hints.Name.displayWidth}"
                                  maximumLength="#{bindings.Vctat_RO_Status1.hints.Name.precision}"
                                  shortDesc="#{bindings.Vctat_RO_Status1.hints.Name.tooltip}" id="it2"
                                  contentStyle='text-align: center'>
                      <f:validator binding="#{row.bindings.Name.validator}"/>
                    </af:inputText>
                  </af:column>
                 
                  <af:column headerText="RO Status" id="c4" sortable="true" filterable="true">
                   <f:facet name="filter">
                                          <af:inputText value="#{vs.filterCriteria.RoStatus}"
                                          id="it6" placeholder="Search Status">
                           </af:inputText>
                             </f:facet>
                    <af:selectBooleanCheckbox value="#{row.bindings.RoStatus.inputValue}"
                                              label="#{row.bindings.RoStatus.label}" autoSubmit="true"
                                              shortDesc="#{bindings.Vctat_RO_Status1.hints.RoStatus.tooltip}"
                                              id="sbc1"/>
                  </af:column>
                </af:table>
              </af:showDetailItem>
            </af:panelTabbed>
          </af:gridCell>
        </af:gridRow>
        <af:gridRow marginTop="5px" height="auto" id="gr3">
