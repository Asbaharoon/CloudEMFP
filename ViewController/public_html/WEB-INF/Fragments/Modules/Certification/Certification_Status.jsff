                                  columns="#{bindings.TblCertAppMasterView1.hints.Cnic.displayWidth}"
                                  maximumLength="#{bindings.TblCertAppMasterView1.hints.Cnic.precision}"
                                  shortDesc="#{bindings.TblCertAppMasterView1.hints.Cnic.tooltip}" id="it2">
                      <f:validator binding="#{row.bindings.Cnic.validator}"/>
                    </af:inputText>
                  </af:column>
                  <af:column sortProperty="#{bindings.TblCertAppMasterView1.hints.Mobile.name}" filterable="true"
                             sortable="true" headerText="#{bindings.TblCertAppMasterView1.hints.Mobile.label}" id="c7">
                    <af:inputText value="#{row.bindings.Mobile.inputValue}"
                                  label="#{bindings.TblCertAppMasterView1.hints.Mobile.label}"
                                  required="#{bindings.TblCertAppMasterView1.hints.Mobile.mandatory}"
                                  columns="#{bindings.TblCertAppMasterView1.hints.Mobile.displayWidth}"
                                  maximumLength="#{bindings.TblCertAppMasterView1.hints.Mobile.precision}"
                                  shortDesc="#{bindings.TblCertAppMasterView1.hints.Mobile.tooltip}" id="it3">
                      <f:validator binding="#{row.bindings.Mobile.validator}"/>
                    </af:inputText>
