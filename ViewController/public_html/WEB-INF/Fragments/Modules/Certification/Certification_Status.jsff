                                  shortDesc="#{bindings.TblCertAppMasterView1.hints.CertAppMasterId.tooltip}" id="it1">
                      <f:validator binding="#{row.bindings.CertAppMasterId.validator}"/>
                      <af:convertNumber groupingUsed="false"
                                        pattern="#{bindings.TblCertAppMasterView1.hints.CertAppMasterId.format}"/>
                    </af:inputText>
                  </af:column>
                  <af:column sortProperty="#{bindings.TblCertAppMasterView1.hints.CertificationId.name}"
                             filterable="true" sortable="true"
                             headerText="#{bindings.TblCertAppMasterView1.hints.CertificationId.label}" id="c2">
                    <af:selectOneChoice value="#{row.bindings.CertificationId.inputValue}"
                                        label="#{row.bindings.CertificationId.label}"
                                        required="#{bindings.TblCertAppMasterView1.hints.CertificationId.mandatory}"
                                        shortDesc="#{bindings.TblCertAppMasterView1.hints.CertificationId.tooltip}"
                                        id="soc1">
                      <f:selectItems value="#{row.bindings.CertificationId.items}" id="si1"/>
                      <f:validator binding="#{row.bindings.CertificationId.validator}"/>
                    </af:selectOneChoice>
                  </af:column>
                  <af:column sortProperty="#{bindings.TblCertAppMasterView1.hints.AppStatus.name}" filterable="true"
                             sortable="true" headerText="#{bindings.TblCertAppMasterView1.hints.AppStatus.label}"
                             id="c3">
                    <af:selectOneChoice value="#{row.bindings.AppStatus.inputValue}"
                                        label="#{row.bindings.AppStatus.label}"
                                        required="#{bindings.TblCertAppMasterView1.hints.AppStatus.mandatory}"
                                        shortDesc="#{bindings.TblCertAppMasterView1.hints.AppStatus.tooltip}" id="soc2">
                      <f:selectItems value="#{row.bindings.AppStatus.items}" id="si2"/>
                      <f:validator binding="#{row.bindings.AppStatus.validator}"/>
                    </af:selectOneChoice>
                  </af:column>
