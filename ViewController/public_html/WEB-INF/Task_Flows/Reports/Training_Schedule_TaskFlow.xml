    <default-activity>Training_Schedule</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>training_sch_report_bean</managed-bean-name>
      <managed-bean-class>view.report.training_sch_report</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="Training_Schedule">
      <page>/WEB-INF/Fragments/Reports/Training_Schedule.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
